import java.util.Scanner;
public class stark {
    public static void main(String args[]){
        StringBuffer str=new StringBuffer("Tony Stark");
        System.out.println(str);
        str.append(", Genius,Billionaire");
        System.out.println(str);
        str.insert(0,"I am");
        System.out.println(str);
        str.delete(5,10);
        System.out.println(str);
        str.reverse();
        System.out.println(str);
        str.replace(0, 3,"AA");
        System.out.println(str);
    }
}
