class Animal
{
    String color="red";
}
class Bird
{
    String color="purple";
}
class Dog extends Bird
{
   String color="White";
   void display()
   {
       System.out.println(color);
       System.out.println(super.color);
   }
}
public class Main
{
    public static void main(String[]args){
    Dog obj=new Dog();
    obj.display();
}
}
