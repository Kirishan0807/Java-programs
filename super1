class bird
{
    String color="red";
void eat()
{
    System.out.println("Eating");
}}
class Dog extends bird
{
   void bark()
   {
       System.out.println("barking");
   }
   void display(){
       super.eat();
       bark();
   }}
public class Main
{
    public static void main(String[]args){
    Dog obj=new Dog();
    obj.display();
}
}
